                            <subviews>
                                <label opaque="NO" clipsSubviews="YES" userInteractionEnabled="NO" contentMode="left" lineBreakMode="wordWrap" numberOfLines="0" baselineAdjustment="alignBaselines" minimumFontSize="10" id="26">
                                    <rect key="frame" x="20" y="32" width="390" height="141"/>
                                    <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMaxY="YES"/>
                                    <string key="text">In Phrase Frenzy, two teams square off in a fast-paced word-guessing version of “hot potato.” The device is passed around from person to person (on alternating teams), and each player gives verbal and physical clues in order to get their teammates to guess the word or phrase they are given before time expires. Play as fast as possible; you don’t want to be the one stuck with the device when the buzzer goes off!</string>
                                    <fontDescription key="fontDescription" type="system" pointSize="14"/>
                                    <color key="textColor" red="0.32210782170295715" green="0.32209819555282593" blue="0.32210367918014526" alpha="1" colorSpace="custom" customColorSpace="sRGB"/>
                                    <nil key="highlightedColor"/>
                                </label>
                                <label opaque="NO" clipsSubviews="YES" userInteractionEnabled="NO" contentMode="left" text="GAME SET-UP" lineBreakMode="tailTruncation" baselineAdjustment="alignBaselines" minimumFontSize="10" id="27">
